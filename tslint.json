{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": [true, "never-prefix"],
      "member-access": [true, "no-public"],
      "member-ordering": [true, {"order": "fields-first"}],
      "no-console": false,
      "quotemark": [true, "single"],
      "triple-equals": false
    },
    "rulesDirectory": []
}
