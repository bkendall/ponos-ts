{
  "name": "ponos-ts",
  "version": "0.0.0",
  "description": "Typescript port of Ponos",
  "scripts": {
    "build-ts": "tsc",
    "style": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha $npm_package_options_mocha test/**/*.ts",
    "test-all": "npm run style && npm run build-ts && npm run test",
    "test-functional": "mocha $npm_package_options_mocha test-functional/**/*.ts",
    "watch-test": "mocha $npm_package_options_mocha -w --watch-extensions ts test/**/*.ts",
    "watch-ts": "tsc --watch"
  },
  "options": {
    "mocha": "--require ts-node/register --require resources/mocha-bootstrap.ts --reporter dot"
  },
  "author": "Bryan Kendall <bryan@bryankendall.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/amqplib": "^0.5.9",
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "bluebird": "^3.5.3"
  }
}
